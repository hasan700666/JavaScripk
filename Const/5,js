//Scope 
//let has block scope
//const has block scope
//var can not has block scope

//EX of let

let x = 1;  //hare x valu is 1

{
    let x = 10;  //hare x valu is 10
}

//hare x valu is 1


//EX of const

const y = 10; //hare y valu is 10

{
    const y = 20;  //hare y valu is 20
}

//hare y valu is 10


// EX of var

var z = 100; //hare z valu is 100

{
    var z = 200; //hare z valu is 200
}

//But hare z valu is 200


